      <!-- body-content -->
      <div class="body-content clearfix" >

        <div class="block-section bg-color4">
          <div class="container">
            <div class="panel panel-md">
              <div class="panel-body">
                <div class="row">
                  <div class="col-md-12" style="padding-left:40px;padding-right:40px;padding-bottom : 40px;">
                    <!-- buttons top -->
                   
                    <!-- end buttons top -->

                    <div class="white-space-10"></div>
                    <h3 class="text-center">Créez votre compte pour publier des offres d'emploi</h4>
                      <hr/>
                      {% if error is defined %}
                      {% if error %}
                     <h5 style="color:red;font-weight:bold;">Cette adresse e-mail existe déjà.</h5>
                      {% endif %}
                      {% endif %}
                    <!-- form login  -->
                    
                    <form action={{path('registration-submit')}} method="POST" >
                  <div class="form-group">
                    <label>Nom de votre entreprise</label>
                    <input type="text" class="form-control " name="company" placeholder="le nom de votre entreprise">
                  </div>
                  <div class="form-group">
                    <label>Email de votre entrprise</label>
                    <input type="text" class="form-control " name="email" placeholder="Ex : exemple@exemple.com">
                  </div>
                  <div class="form-group">
                    <label>Téléphone de votre entreprise</label>
                    <input type="text" class="form-control " name="telephone" placeholder="">
                  </div>
                  <div class="form-group">
                    <label>Adresse</label>
                    <div class="color-white-mute"><small>ici l'adresse en detail de l'entreprise .</small></div>
                    <textarea class="form-control" rows="6" name="adresse" ></textarea>
                  </div>
                  <div class="row">
                  <div class="form-group col-md-4">
                    <label>Lieu</label>
                    <input type="text" class="form-control "  name="location" placeholder="EX: Rabat">
                    
                  </div >
                  <div class="form-group col-md-8">
                    <label>Secteur</label>
                    <select class="form-control" name="category">
                      <option>L'industrie</option>
                      <option>Technologie de l'information</option>
                      <option>Education</option>
                      <option>Banque et Finance</option>
                      <option>Médias</option>
                    </select>
                  </div>
                  </div>

                   <div class="form-group">
                    <label>Mot de passe</label>
                    <input type="password" class="form-control " name="password" >
                  </div>

                  <div class="form-group">
                    <label>confirmer votre mot de passe</label>
                    <input type="password" class="form-control " name="password-confirmation" >
                  </div>

                  <div class="form-group ">
                    <button id="submit" class="btn btn-t-primary btn-theme" type="submit" >Valider</button>
                    <button class="btn btn-theme btn-line soft" type="reset" >Vider</button>

                  </div>


                </form> <!-- end form post a job -->
                  </div>
                </div>
              </div>
            </div>

